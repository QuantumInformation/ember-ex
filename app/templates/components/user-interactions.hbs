<table id="interactions">
  <th id="interactiveElements">Interactive elements</th>
  <th>Event that generates code</th>
  <th id="mutationColumn">Mutation (if any)</th>
  <tr>
    <td>
      <button>No side effect</button>
    </td>
    <td>onclick</td>
    <td></td>
  </tr>
  <tr>
    <td>
      <button class="makeDiv test2 ">Creates a div</button>
      <button id="removeDiv">Removes the div</button>
    </td>
    <td>onclick</td>
    <td id="buttonClickMutation"></td>
  </tr>
  <tr>
    <td><input type="text" id="input">
    </td>
    <td>focusout</td>
    <td id="inputMutation">Initial Value input</td>
  </tr>
  <tr>
    <td><select name="" id="select">
      <option value="value1">Option 1</option>
      <option value="value2">Option 2</option>
      <option value="value3">Option 3</option>
    </select></td>
    <td>change</td>
    <td id="selectMutation">Initial Value select</td>
  </tr>
  <tr>
    <td>
      <div class="dropDiv" ondrop="drop(event)" ondragover="allowDrop(event)">
        <img id="drag1" src="../logo.png" draggable="true"
             ondragstart="drag(event)" width="30">
      </div>
    </td>
    <td>ondrop</td>
    <td>
      <div class="dropDiv" ondrop="drop(event)" ondragover="allowDrop(event)"></div>
    </td>
  </tr>
</table>